<?xml version="1.0" encoding="UTF-8"?>
<typeSystemDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <name>scene</name>
  <description/>
  <version>1.0</version>
  <vendor/>
  <imports>
    <!-- THESE IMPORTS WILL BE AUTOMATICALLY GENERATED BY A SCRIPT -->
    <import location="core_types.xml"/>
    <import location="pcl_types.xml"/>
    <import location="cv_types.xml"/>
  </imports>
  <types>
    <typeDescription>
      <name>rs.scene.ObjectHypothesis</name>
      <description/>
      <supertypeName>rs.core.Identifiable</supertypeName>
      <features>
        <featureDescription>
          <name>annotations</name>
          <description/>
          <rangeTypeName>uima.cas.FSList</rangeTypeName>
          <elementType>rs.core.Annotation</elementType>
          <multipleReferencesAllowed>true</multipleReferencesAllowed>
        </featureDescription>
        <featureDescription>
          <name>points</name>
          <description/>
          <rangeTypeName>rs.pcl.ClusterPoints</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>centroid</name>
          <description>Centroid of a cluster</description>
          <rangeTypeName>uima.cas.FloatArray</rangeTypeName>
          <multipleReferencesAllowed>false</multipleReferencesAllowed>
        </featureDescription>
        <featureDescription>
          <name>rois</name>
          <description/>
          <rangeTypeName>rs.cv.ImageROI</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>source</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>

    <typeDescription>
      <name>rs.scene.MergedCluster</name>
      <description/>
      <supertypeName>rs.scene.ObjectHypothesis</supertypeName>
    </typeDescription>

    <typeDescription>
      <name>rs.scene.Object</name>
      <description/>
      <supertypeName>rs.scene.ObjectHypothesis</supertypeName>
      <features>
        <featureDescription>
          <name>uid</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>lastSeen</name>
          <description/>
          <rangeTypeName>uima.cas.Long</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>wasSeen</name>
          <description>True if the object was seen in the last frame</description>
          <rangeTypeName>uima.cas.Boolean</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>inView</name>
          <description>True if the object should be in camera View</description>
          <rangeTypeName>uima.cas.Boolean</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>disappeared</name>
          <description>True if the object was not seen while it should have been in view</description>
          <rangeTypeName>uima.cas.Boolean</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>clusters</name>
          <description/>
          <rangeTypeName>uima.cas.StringArray</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
  </types>
</typeSystemDescription>
