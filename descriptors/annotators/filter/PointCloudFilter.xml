<?xml version="1.0" encoding="UTF-8"?>
<taeDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.cpp</frameworkImplementation>
  <primitive>true</primitive>
  <annotatorImplementationName>rs_PointCloudFilter</annotatorImplementationName>
  <analysisEngineMetaData>
    <name>PointCloudFilter</name>
    <description>Filters a point cloud given X,Y,and Z limits while keeping it organized</description>
    <version>1.0</version>
    <configurationParameters>

    <configurationParameter>
        <name>minX</name>
        <type>Float</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>maxX</name>
        <type>Float</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
      </configurationParameter>

      <configurationParameter>
        <name>minY</name>
        <type>Float</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>maxY</name>
        <type>Float</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
      </configurationParameter>

      <configurationParameter>
        <name>minZ</name>
        <type>Float</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>maxZ</name>
        <type>Float</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
      </configurationParameter>


    </configurationParameters>
    <configurationParameterSettings>
      <nameValuePair>
        <name>maxZ</name>
        <value>
          <float>2</float>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>minZ</name>
        <value>
          <float>0</float>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>maxX</name>
        <value>
          <float>1.2</float>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>minX</name>
        <value>
          <float>-1.2</float>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>maxY</name>
        <value>
          <float>1.2</float>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>minY</name>
        <value>
          <float>-1.2</float>
        </value>
      </nameValuePair>
    </configurationParameterSettings>
    <typeSystemDescription>
      <imports>
        <import location="../../typesystem/all_types.xml"/>
      </imports>
    </typeSystemDescription>
    <typePriorities/>
    <capabilities>
      <capability>
	<inputs/>
        <outputs>
          <type allAnnotatorFeatures="true">rs.pcl.FilteredPointCloud</type>
        </outputs>
        <inputSofas>
          <sofaName>rs.pcl.PointCloud</sofaName>
        </inputSofas>
        <languagesSupported>
          <language>x-unspecified</language>
        </languagesSupported>
	</capability>
    </capabilities>
    <operationalProperties>
      <modifiesCas>true</modifiesCas>
      <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
      <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
</taeDescription>
