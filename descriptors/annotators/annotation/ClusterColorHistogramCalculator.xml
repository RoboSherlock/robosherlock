<?xml version="1.0" encoding="UTF-8"?><taeDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.cpp</frameworkImplementation>
  <primitive>true</primitive>
  <annotatorImplementationName>rs_ClusterColorHistogramCalculator</annotatorImplementationName>
  <analysisEngineMetaData>
    <name>ClusterColorHistogramCalculator</name>
    <description>RequiresPerceiveColorCapability</description>
    <version>1.0</version>
    <outputDomain>yellow, blue, black, white, red, green, magenta, cyan</outputDomain>
    <configurationParameters>
      <configurationParameter>
        <name>minValueColor</name>
        <type>Integer</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>minSaturationColor</name>
        <type>Integer</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>maxValueBlack</name>
        <type>Integer</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>minValueWhite</name>
        <type>Integer</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>histogramCols</name>
        <type>Integer</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>histogramRows</name>
        <type>Integer</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
      </configurationParameter>
    </configurationParameters>
    <configurationParameterSettings>
      <nameValuePair>
        <name>minValueColor</name>
        <value>
          <integer>60</integer>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>minSaturationColor</name>
        <value>
          <integer>60</integer>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>maxValueBlack</name>
        <value>
          <integer>60</integer>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>minValueWhite</name>
        <value>
          <integer>120</integer>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>histogramCols</name>
        <value>
          <integer>16</integer>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>histogramRows</name>
        <value>
          <integer>16</integer>
        </value>
      </nameValuePair>
    </configurationParameterSettings>
    <typeSystemDescription>
      <imports>
        <import location="../../typesystem/all_types.xml"/>
      </imports>
    </typeSystemDescription>
    <capabilities>
      <capability>
        <inputs/>
        <outputs>
          <type allAnnotatorFeatures="true">rs.annotation.ColorHistogram</type>
          <type allAnnotatorFeatures="true">rs.annotation.SemanticColor</type>
        </outputs>
        <inputSofas>
          <sofaName>rs.scene.MergedCluster</sofaName>
        </inputSofas>
        <languagesSupported>
          <language>x-unspecified</language>
        </languagesSupported>
      </capability>
    </capabilities>
    <operationalProperties>
      <modifiesCas>true</modifiesCas>
      <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
      <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
</taeDescription>
