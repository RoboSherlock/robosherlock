<?xml version="1.0" encoding="UTF-8"?>
<taeDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.cpp</frameworkImplementation>
  <primitive>true</primitive>
  <annotatorImplementationName>rs_rotationalSymmetryAnnotator</annotatorImplementationName>
  <analysisEngineMetaData>
    <name>RotationalSymmetryAnnotator</name>
    <description>Annotate rotational symmetry from point cloud segments.</description>
    <version>1.0</version>
    <vendor/>
    <configurationParameters>
        <configurationParameter>
            <name>rotSymAnn_min_fit_angle</name>
            <type>Float</type>
            <description>Lower bound range for scaling symmetric scoring</description>
            <multiValued>false</multiValued>
            <mandatory>true</mandatory>
        </configurationParameter>
        <configurationParameter>
            <name>rotSymAnn_max_fit_angle</name>
            <type>Float</type>
            <description>Upper bound range for scaling symmetric scoring</description>
            <multiValued>false</multiValued>
            <mandatory>true</mandatory>
        </configurationParameter>
        <configurationParameter>
            <name>rotSymAnn_min_occlusion_dist</name>
            <type>Float</type>
            <description>Lower bound for measuring occlusion in a segment</description>
            <multiValued>false</multiValued>
            <mandatory>true</mandatory>
        </configurationParameter>
        <configurationParameter>
            <name>rotSymAnn_max_occlusion_dist</name>
            <type>Float</type>
            <description>Upper bound for measuring occlusion in a segment</description>
            <multiValued>false</multiValued>
            <mandatory>true</mandatory>
        </configurationParameter>
        <configurationParameter>
            <name>rotSymAnn_max_sym_score</name>
            <type>Float</type>
            <description>Maximum bound for passing segments more symmetric property </description>
            <multiValued>false</multiValued>
            <mandatory>true</mandatory>
        </configurationParameter>
        <configurationParameter>
            <name>rotSymAnn_max_occlusion_score</name>
            <type>Float</type>
            <description>Maximum bound for passing segments with enough point closeness </description>
            <multiValued>false</multiValued>
            <mandatory>true</mandatory>
        </configurationParameter>
        <configurationParameter>
            <name>rotSymAnn_max_perpendicular_score</name>
            <type>Float</type>
            <description>Some trick for keeping more valued segments</description>
            <multiValued>false</multiValued>
            <mandatory>true</mandatory>
        </configurationParameter>
        <configurationParameter>
            <name>rotSymAnn_min_coverage_score</name>
            <type>Float</type>
            <description>Coverage scoring for a segment, if segment does not cover over this threshold based on symmetry axis, segment is filtered out</description>
            <multiValued>false</multiValued>
            <mandatory>true</mandatory>
        </configurationParameter>
        <configurationParameter>
            <name>dist_map_resolution</name>
            <type>Float</type>
            <multiValued>false</multiValued>
            <mandatory>true</mandatory>
        </configurationParameter>
        <configurationParameter>
            <name>boundaryRadiusSearch</name>
            <type>Float</type>
            <multiValued>false</multiValued>
            <mandatory>true</mandatory>
        </configurationParameter>
        <configurationParameter>
            <name>boundaryAngleThreshold</name>
            <type>Float</type>
            <multiValued>false</multiValued>
            <mandatory>true</mandatory>
        </configurationParameter>
        <configurationParameter>
            <name>max_angle_diff</name>
            <type>Float</type>
            <description>Maximum angle between two symmetry to be consider two distinct sym</description>
            <multiValued>false</multiValued>
            <mandatory>true</mandatory>
        </configurationParameter>
        <configurationParameter>
            <name>max_dist_diff</name>
            <type>Float</type>
            <description>Maximum distance between two symmetry to be consider two distinct sym</description>
            <multiValued>false</multiValued>
            <mandatory>true</mandatory>
        </configurationParameter>
    </configurationParameters>
    <configurationParameterSettings>
        <nameValuePair>
            <name>rotSymAnn_min_fit_angle</name>
            <value>
                <float>0.175f</float>
            </value>
        </nameValuePair>
        <nameValuePair>
            <name>rotSymAnn_max_fit_angle</name>
            <value>
                <float>1.047f</float>
            </value>
        </nameValuePair>
        <nameValuePair>
            <name>rotSymAnn_min_occlusion_dist</name>
            <value>
                <float>0.01f</float>
            </value>
        </nameValuePair>
        <nameValuePair>
            <name>rotSymAnn_max_occlusion_dist</name>
            <value>
                <float>0.03f</float>
            </value>
        </nameValuePair>
        <nameValuePair>
            <name>rotSymAnn_max_sym_score</name>
            <value>
                <float>0.02f</float>
            </value>
        </nameValuePair>
        <nameValuePair>
            <name>rotSymAnn_max_occlusion_score</name>
            <value>
                <float>0.012f</float>
            </value>
        </nameValuePair>
        <nameValuePair>
            <name>rotSymAnn_max_perpendicular_score</name>
            <value>
                <float>0.6f</float>
            </value>
        </nameValuePair>
        <nameValuePair>
            <name>rotSymAnn_min_coverage_score</name>
            <value>
                <float>0.3f</float>
            </value>
        </nameValuePair>
        <nameValuePair>
            <name>dist_map_resolution</name>
            <value>
                <float>0.005f</float>
            </value>
        </nameValuePair>
        <nameValuePair>
            <name>boundaryRadiusSearch</name>
            <value>
                <float>0.01f</float>
            </value>
        </nameValuePair>
        <nameValuePair>
            <name>boundaryAngleThreshold</name>
            <value>
                <float>2.356f</float>
            </value>
        </nameValuePair>
        <nameValuePair>
            <name>max_angle_diff</name>
            <value>
                <float>0.174f</float>
            </value>
        </nameValuePair>
        <nameValuePair>
            <name>max_dist_diff</name>
            <value>
                <float>0.01f</float>
            </value>
        </nameValuePair>
    </configurationParameterSettings>
    <typeSystemDescription>
        <imports>
            <import location="../../typesystem/all_types.xml"/>
        </imports>
    </typeSystemDescription>
    <capabilities>
        <capability>
            <inputs/>
            <outputs/>
            <languagesSupported>
                <language>x-unspecified</language>
            </languagesSupported>
        </capability>
    </capabilities>
    <operationalProperties>
        <modifiesCas>true</modifiesCas>
        <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
        <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
</taeDescription>
