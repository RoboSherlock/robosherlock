<?xml version="1.0" encoding="UTF-8"?>
<taeDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.cpp</frameworkImplementation>
  <primitive>true</primitive>
  <annotatorImplementationName>rs_overSegmentationAnnotator</annotatorImplementationName>
  <analysisEngineMetaData>
    <name>OverSegmentationAnnotator</name>
    <description>Over segment point cloud and produce point cloud clusters of same plane.</description>
    <version>1.0</version>
    <vendor/>
    <configurationParameters>
        <configurationParameter>
            <name>minNormalThreshold</name>
            <type>Float</type>
            <description>Lower bound of Smoothness Threshold angle range, for parallel running OverSegmentation with different Smoothness Threshold</description>
            <multiValued>false</multiValued>
            <mandatory>false</mandatory>
        </configurationParameter>
        <configurationParameter>
            <name>maxNormalThreshold</name>
            <type>Float</type>
            <description>Upper bound of Smoothness Threshold angle range, for parallel running OverSegmentation with different Smoothness Threshold</description>
            <multiValued>false</multiValued>
            <mandatory>false</mandatory>
        </configurationParameter>
        <configurationParameter>
            <name>curvatureThreshold</name>
            <type>Float</type>
            <multiValued>false</multiValued>
            <mandatory>false</mandatory>
        </configurationParameter>
        <configurationParameter>
            <name>overlapThreshold</name>
            <type>Float</type>
            <description>Overlap Ratio Threshold (Intersection Points over Union Points) criteria to merge two segments</description>
            <multiValued>false</multiValued>
            <mandatory>false</mandatory>
        </configurationParameter>
        <configurationParameter>
            <name>minClusterSize</name>
            <type>Integer</type>
            <description>Min cluster size to filter out too small segments</description>
            <multiValued>false</multiValued>
            <mandatory>false</mandatory>
        </configurationParameter>
        <configurationParameter>
            <name>maxClusterSize</name>
            <type>Integer</type>
            <description>Max cluster size to filter out too large segments</description>
            <multiValued>false</multiValued>
            <mandatory>false</mandatory>
        </configurationParameter>
        <configurationParameter>
            <name>neighborNumber</name>
            <type>Integer</type>
            <description>Total number of searching surrounding neighbor points</description>
            <multiValued>false</multiValued>
            <mandatory>false</mandatory>
        </configurationParameter>
        <configurationParameter>
            <name>numSegmentation</name>
            <type>Integer</type>
            <description>Number of instances to parallel run OverSegmentation</description>
            <multiValued>false</multiValued>
            <mandatory>false</mandatory>
        </configurationParameter>
    </configurationParameters>
    <configurationParameterSettings>
        <nameValuePair>
            <name>minNormalThreshold</name>
            <value>
                <float>0.13</float>
            </value>
        </nameValuePair>
        <nameValuePair>
            <name>maxNormalThreshold</name>
            <value>
                <float>0.15</float>
            </value>
        </nameValuePair>
        <nameValuePair>
            <name>curvatureThreshold</name>
            <value>
                <float>0.5</float>
            </value>
        </nameValuePair>
        <nameValuePair>
            <name>overlapThreshold</name>
            <value>
                <float>0.8</float>
            </value>
        </nameValuePair>
        <nameValuePair>
            <name>minClusterSize</name>
            <value>
                <integer>120</integer>
            </value>
        </nameValuePair>
        <nameValuePair>
            <name>maxClusterSize</name>
            <value>
                <integer>1000000</integer>
            </value>
        </nameValuePair>
        <nameValuePair>
            <name>neighborNumber</name>
            <value>
                <integer>5</integer>
            </value>
        </nameValuePair>
        <nameValuePair>
            <name>numSegmentation</name>
            <value>
                <integer>2</integer>
            </value>
        </nameValuePair>
    </configurationParameterSettings>
    <typeSystemDescription>
        <imports>
            <import location="../../typesystem/all_types.xml"/>
        </imports>
    </typeSystemDescription>
    <capabilities>
        <capability>
            <inputs/>
            <outputs/>
            <languagesSupported>
                <language>x-unspecified</language>
            </languagesSupported>
        </capability>
    </capabilities>
    <operationalProperties>
        <modifiesCas>true</modifiesCas>
        <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
        <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
</taeDescription>
