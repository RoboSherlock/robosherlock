<?xml version="1.0" encoding="UTF-8"?>
<taeDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.cpp</frameworkImplementation>
  <primitive>true</primitive>
  <annotatorImplementationName>rs_rotationalSymmetrySegmentation</annotatorImplementationName>
  <analysisEngineMetaData>
    <name>RotationalSymmetrySegmentation</name>
    <description>Extract cloud segments from rotational symmteries.</description>
    <version>1.0</version>
    <vendor/>
    <configurationParameters>
      <configurationParameter>
          <name>rotSymSeg_isDownsampled</name>
          <type>Boolean</type>
          <description>Option to downsample the cloud for faster processing.</description>
          <multiValued>false</multiValued>
          <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
          <name>downsample_leaf_size</name>
          <type>Float</type>
          <multiValued>false</multiValued>
          <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
          <name>dist_map_resolution</name>
          <type>Float</type>
          <multiValued>false</multiValued>
          <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
          <name>rotSymSeg_adjacency_radius</name>
          <type>Float</type>
          <description>Max radius to find adjacent neighbors of a point</description>
          <multiValued>false</multiValued>
          <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
          <name>rotSymSeg_num_adjacency_neighbors</name>
          <type>Integer</type>
          <description>Number of finding adjacent neighbors of a point</description>
          <multiValued>false</multiValued>
          <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
          <name>adjacency_sigma_convex</name>
          <type>Float</type>
          <description>Please refer Symmetry Segmentation paper. http://www.umiacs.umd.edu/%7Eaecins/projects/symseg/symmetry_segmentation_ICRA16.pdf</description>
          <multiValued>false</multiValued>
          <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
          <name>adjacency_sigma_concave</name>
          <type>Float</type>
          <description>Please refer Symmetry Segmentation paper. http://www.umiacs.umd.edu/%7Eaecins/projects/symseg/symmetry_segmentation_ICRA16.pdf</description>
          <multiValued>false</multiValued>
          <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
          <name>rotSymSeg_adjacency_weight_factor</name>
          <type>Float</type>
          <description>Scaling weight of adjacency neighbors for favoring Region Growing effect</description>
          <multiValued>false</multiValued>
          <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
          <name>rotSymSeg_min_fit_angle</name>
          <type>Float</type>
          <description>Lower bound range for scaling scoring symmetric segments</description>
          <multiValued>false</multiValued>
          <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
          <name>rotSymSeg_max_fit_angle</name>
          <type>Float</type>
          <description>Upper bound range for scaling scoring symmetric segments</description>
          <multiValued>false</multiValued>
          <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
          <name>rotSymSeg_min_occlusion_dist</name>
          <type>Float</type>
          <description>Lower bound for measuring occlusion in a segment</description>
          <multiValued>false</multiValued>
          <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
          <name>rotSymSeg_max_occlusion_dist</name>
          <type>Float</type>
          <description>Upper bound for measuring occlusion in a segment</description>
          <multiValued>false</multiValued>
          <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
          <name>rotSymSeg_max_perpendicular_angle</name>
          <type>Float</type>
          <multiValued>false</multiValued>
          <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
          <name>rotSymSeg_fg_weight_factor</name>
          <type>Float</type>
          <description>Scaling weight of foreground point for keeping more points in segment</description>
          <multiValued>false</multiValued>
          <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
          <name>rotSymSeg_bg_weight_factor</name>
          <type>Float</type>
          <description>Scaling weight of background point for cutting more points in segment</description>
          <multiValued>false</multiValued>
          <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
          <name>rotSymSeg_max_sym_score</name>
          <type>Float</type>
          <description>Maximum bound for passing segments more symmetric property </description>
          <multiValued>false</multiValued>
          <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
          <name>rotSymSeg_max_occlusion_score</name>
          <type>Float</type>
          <description>Maximum bound for passing segments with enough point closeness </description>
          <multiValued>false</multiValued>
          <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
          <name>rotSymSeg_max_cut_score</name>
          <type>Float</type>
          <description>Maximum bound for passing segments enough cut score per segment point</description>
          <multiValued>false</multiValued>
          <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
          <name>min_segment_size</name>
          <type>Integer</type>
          <description>Minimum segment size to pass</description>
          <multiValued>false</multiValued>
          <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
          <name>overlap_threshold</name>
          <type>Float</type>
          <description>Overlap Ratio between two segment to be merged</description>
          <multiValued>false</multiValued>
          <mandatory>true</mandatory>
      </configurationParameter>
    </configurationParameters>
    <configurationParameterSettings>
      <nameValuePair>
          <name>rotSymSeg_isDownsampled</name>
          <value>
              <boolean>true</boolean>
          </value>
      </nameValuePair>
      <nameValuePair>
          <name>downsample_leaf_size</name>
          <value>
              <float>0.005f</float>
          </value>
      </nameValuePair>
      <nameValuePair>
          <name>dist_map_resolution</name>
          <value>
              <float>0.005f</float>
          </value>
      </nameValuePair>
      <nameValuePair>
          <name>rotSymSeg_adjacency_radius</name>
          <value>
              <float>0.01f</float>
          </value>
      </nameValuePair>
      <nameValuePair>
          <name>rotSymSeg_num_adjacency_neighbors</name>
          <value>
              <integer>9</integer>
          </value>
      </nameValuePair>
      <nameValuePair>
          <name>adjacency_sigma_convex</name>
          <value>
              <float>2.0f</float>
          </value>
      </nameValuePair>
      <nameValuePair>
          <name>adjacency_sigma_concave</name>
          <value>
              <float>0.15f</float>
          </value>
      </nameValuePair>
      <nameValuePair>
          <name>rotSymSeg_adjacency_weight_factor</name>
          <value>
              <float>3.0f</float>
          </value>
      </nameValuePair>
      <nameValuePair>
          <name>rotSymSeg_min_fit_angle</name>
          <value>
              <float>0.0f</float>
          </value>
      </nameValuePair>
      <nameValuePair>
          <name>rotSymSeg_max_fit_angle</name>
          <value>
              <float>1.155f</float>
          </value>
      </nameValuePair>
      <nameValuePair>
          <name>rotSymSeg_min_occlusion_dist</name>
          <value>
              <float>0.005f</float>
          </value>
      </nameValuePair>
      <nameValuePair>
          <name>rotSymSeg_max_occlusion_dist</name>
          <value>
              <float>0.155f</float>
          </value>
      </nameValuePair>
      <nameValuePair>
          <name>rotSymSeg_max_perpendicular_angle</name>
          <value>
              <float>1.255f</float>
          </value>
      </nameValuePair>
      <nameValuePair>
          <name>rotSymSeg_fg_weight_factor</name>
          <value>
              <float>2.0f</float>
          </value>
      </nameValuePair>
      <nameValuePair>
          <name>rotSymSeg_bg_weight_factor</name>
          <value>
              <float>1.0f</float>
          </value>
      </nameValuePair>
      <nameValuePair>
          <name>rotSymSeg_max_sym_score</name>
          <value>
              <float>0.4f</float>
          </value>
      </nameValuePair>
      <nameValuePair>
          <name>rotSymSeg_max_occlusion_score</name>
          <value>
              <float>0.03f</float>
          </value>
      </nameValuePair>
      <nameValuePair>
          <name>rotSymSeg_max_cut_score</name>
          <value>
              <float>0.3f</float>
          </value>
      </nameValuePair>
      <nameValuePair>
          <name>min_segment_size</name>
          <value>
              <integer>400</integer>
          </value>
      </nameValuePair>
      <nameValuePair>
          <name>overlap_threshold</name>
          <value>
              <float>0.3f</float>
          </value>
      </nameValuePair>
    </configurationParameterSettings>
    <typeSystemDescription>
        <imports>
            <import location="../../typesystem/all_types.xml"/>
        </imports>
    </typeSystemDescription>
    <capabilities>
        <capability>
            <inputs/>
            <outputs/>
            <languagesSupported>
                <language>x-unspecified</language>
            </languagesSupported>
        </capability>
    </capabilities>
    <operationalProperties>
        <modifiesCas>true</modifiesCas>
        <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
        <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
</taeDescription>
