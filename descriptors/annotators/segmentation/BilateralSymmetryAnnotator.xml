<?xml version="1.0" encoding="UTF-8"?>
<taeDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.cpp</frameworkImplementation>
  <primitive>true</primitive>
  <annotatorImplementationName>rs_bilateralSymmetryAnnotator</annotatorImplementationName>
  <analysisEngineMetaData>
    <name>BilateralSymmetryAnnotator</name>
    <description>Detect bilateral symmetries from cloud segments.</description>
    <version>1.0</version>
    <vendor/>
    <configurationParameters>
      <configurationParameter>
          <name>bilSymAnn_isDownsampled</name>
          <type>Boolean</type>
          <description>Option to downsample the cloud for faster processing.</description>
          <multiValued>false</multiValued>
          <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
          <name>naive_detection</name>
          <type>Boolean</type>
          <description>Option to use naive basis vectors of point cloud as raw symmetries</description>
          <multiValued>false</multiValued>
          <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
          <name>downsample_voxel_size</name>
          <type>Float</type>
          <multiValued>false</multiValued>
          <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
          <name>angle_division</name>
          <type>Integer</type>
          <description>Angle denominator for generating symmetry axes over hemisphere</description>
          <multiValued>false</multiValued>
          <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
          <name>dist_map_resolution</name>
          <type>Float</type>
          <multiValued>false</multiValued>
          <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
          <name>correspondence_search_radius</name>
          <type>Float</type>
          <description>Angle denominator for generating symmetry axes over hemisphere</description>
          <multiValued>false</multiValued>
          <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
          <name>correspondence_max_normal_fit_error</name>
          <type>Float</type>
          <description>Maximum normal difference for evaluating correspondence of two point over symmetry plane</description>
          <multiValued>false</multiValued>
          <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
          <name>correspondence_min_sym_dist</name>
          <type>Float</type>
          <description>Minimum distance between two point over symmetry plane to be correspondence</description>
          <multiValued>false</multiValued>
          <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
          <name>correspondence_max_sym_reflected_dist</name>
          <type>Float</type>
          <description>Maximum distance between reflected point and other point over symmetry plane to be correspondence</description>
          <multiValued>false</multiValued>
          <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
          <name>refine_max_iteration</name>
          <type>Integer</type>
          <description>Iteration to refine symmetries based on Levenberg-Maxquart algorithm</description>
          <multiValued>false</multiValued>
          <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
          <name>refine_min_inlier_sym_score</name>
          <type>Float</type>
          <description>Lower bound range for scaling scoring symmetric segments</description>
          <multiValued>false</multiValued>
          <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
          <name>refine_max_inlier_sym_score</name>
          <type>Float</type>
          <description>Upper bound range for scaling scoring symmetric segments</description>
          <multiValued>false</multiValued>
          <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
          <name>bilSymAnn_min_occlusion_dist</name>
          <type>Float</type>
          <description>Lower bound for measuring occlusion in a segment</description>
          <multiValued>false</multiValued>
          <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
          <name>bilSymAnn_max_occlusion_dist</name>
          <type>Float</type>
          <description>Upper bound for measuring occlusion in a segment</description>
          <multiValued>false</multiValued>
          <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
          <name>bilSymAnn_max_occlusion_score</name>
          <type>Float</type>
          <description>Maximum bound for passing segments with enough point closeness </description>
          <multiValued>false</multiValued>
          <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
          <name>min_segment_inlier_score</name>
          <type>Float</type>
          <description>Minimum bound for passing segments more symmetric property </description>
          <multiValued>false</multiValued>
          <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
          <name>min_corres_inlier_score</name>
          <type>Float</type>
          <description>Minimum bound for passing segments more symmetric property over correspondences size</description>
          <multiValued>false</multiValued>
          <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
          <name>sym_angle_diff</name>
          <type>Float</type>
          <description>Maximum angle between two symmetry to be consider two distinct sym</description>
          <multiValued>false</multiValued>
          <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
          <name>sym_dist_diff</name>
          <type>Float</type>
          <description>Maximum distance between two symmetry to be consider two distinct sym</description>
          <multiValued>false</multiValued>
          <mandatory>true</mandatory>
      </configurationParameter>
    </configurationParameters>
    <configurationParameterSettings>
      <nameValuePair>
          <name>bilSymAnn_isDownsampled</name>
          <value>
              <boolean>true</boolean>
          </value>
      </nameValuePair>
      <nameValuePair>
          <name>naive_detection</name>
          <value>
              <boolean>true</boolean>
          </value>
      </nameValuePair>
      <nameValuePair>
          <name>downsample_voxel_size</name>
          <value>
              <float>0.005f</float>
          </value>
      </nameValuePair>
      <nameValuePair>
          <name>angle_division</name>
          <value>
              <integer>2</integer>
          </value>
      </nameValuePair>
      <nameValuePair>
          <name>dist_map_resolution</name>
          <value>
              <float>0.005f</float>
          </value>
      </nameValuePair>
      <nameValuePair>
          <name>correspondence_search_radius</name>
          <value>
              <float>0.01f</float>
          </value>
      </nameValuePair>
      <nameValuePair>
          <name>correspondence_max_normal_fit_error</name>
          <value>
              <float>0.174f</float>
          </value>
      </nameValuePair>
      <nameValuePair>
          <name>correspondence_min_sym_dist</name>
          <value>
              <float>0.02f</float>
          </value>
      </nameValuePair>
      <nameValuePair>
          <name>correspondence_max_sym_reflected_dist</name>
          <value>
              <float>0.01f</float>
          </value>
      </nameValuePair>
      <nameValuePair>
          <name>refine_max_iteration</name>
          <value>
              <integer>20</integer>
          </value>
      </nameValuePair>
      <nameValuePair>
          <name>refine_min_inlier_sym_score</name>
          <value>
              <float>0.174f</float>
          </value>
      </nameValuePair>
      <nameValuePair>
          <name>refine_max_inlier_sym_score</name>
          <value>
              <float>0.232f</float>
          </value>
      </nameValuePair>
      <nameValuePair>
          <name>bilSymAnn_min_occlusion_dist</name>
          <value>
              <float>0.01f</float>
          </value>
      </nameValuePair>
      <nameValuePair>
          <name>bilSymAnn_max_occlusion_dist</name>
          <value>
              <float>0.03f</float>
          </value>
      </nameValuePair>
      <nameValuePair>
          <name>bilSymAnn_max_occlusion_score</name>
          <value>
              <float>0.05f</float>
          </value>
      </nameValuePair>
      <nameValuePair>
          <name>min_segment_inlier_score</name>
          <value>
              <float>0.2f</float>
          </value>
      </nameValuePair>
      <nameValuePair>
          <name>min_corres_inlier_score</name>
          <value>
              <float>0.7f</float>
          </value>
      </nameValuePair>
      <nameValuePair>
          <name>sym_angle_diff</name>
          <value>
              <float>0.2f</float>
          </value>
      </nameValuePair>
      <nameValuePair>
          <name>sym_dist_diff</name>
          <value>
              <float>0.05f</float>
          </value>
      </nameValuePair>
    </configurationParameterSettings>
    <typeSystemDescription>
        <imports>
            <import location="../../typesystem/all_types.xml"/>
        </imports>
    </typeSystemDescription>
    <capabilities>
        <capability>
            <inputs/>
            <outputs/>
            <languagesSupported>
                <language>x-unspecified</language>
            </languagesSupported>
        </capability>
    </capabilities>
    <operationalProperties>
        <modifiesCas>true</modifiesCas>
        <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
        <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
</taeDescription>
