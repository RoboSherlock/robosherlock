<?xml version="1.0" encoding="UTF-8"?>
<taeDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.cpp</frameworkImplementation>
  <primitive>true</primitive>
  <annotatorImplementationName>rs_bilateralSymmetrySegmentation</annotatorImplementationName>
  <analysisEngineMetaData>
    <name>BilateralSymmetrySegmentation</name>
    <description>Extract cloud segments from bilateral symmteries.</description>
    <version>1.0</version>
    <vendor/>
    <configurationParameters>
      <configurationParameter>
          <name>bilSymSeg_isDownsampled</name>
          <type>Boolean</type>
          <description>Option to downsample the cloud for faster processing.</description>
          <multiValued>false</multiValued>
          <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
          <name>downsample_voxel_size</name>
          <type>Float</type>
          <multiValued>false</multiValued>
          <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
          <name>dist_map_resolution</name>
          <type>Float</type>
          <multiValued>false</multiValued>
          <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
          <name>bilSymSeg_adjacency_radius</name>
          <type>Float</type>
          <description>Max radius to find adjacent neighbors of a point</description>
          <multiValued>false</multiValued>
          <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
          <name>bilSymSeg_num_adjacency_neighbors</name>
          <type>Integer</type>
          <description>Number of finding adjacent neighbors of a point</description>
          <multiValued>false</multiValued>
          <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
          <name>adjacency_sigma_convex</name>
          <type>Float</type>
          <description>Please refer Symmetry Segmentation paper. http://www.umiacs.umd.edu/%7Eaecins/projects/symseg/symmetry_segmentation_ICRA16.pdf</description>
          <multiValued>false</multiValued>
          <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
          <name>adjacency_sigma_concave</name>
          <type>Float</type>
          <description>Please refer Symmetry Segmentation paper. http://www.umiacs.umd.edu/%7Eaecins/projects/symseg/symmetry_segmentation_ICRA16.pdf</description>
          <multiValued>false</multiValued>
          <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
          <name>bilSymSeg_adjacency_weight_factor</name>
          <type>Float</type>
          <description>Scaling weight of adjacency neighbors for favoring Region Growing effect</description>
          <multiValued>false</multiValued>
          <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
          <name>bilSymSeg_min_fit_angle</name>
          <type>Float</type>
          <description>Lower bound range for scaling scoring symmetric segments</description>
          <multiValued>false</multiValued>
          <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
          <name>bilSymSeg_max_fit_angle</name>
          <type>Float</type>
          <description>Upper bound range for scaling scoring symmetric segments</description>
          <multiValued>false</multiValued>
          <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
          <name>bilSymSeg_min_occlusion_dist</name>
          <type>Float</type>
          <description>Lower bound for measuring occlusion in a segment</description>
          <multiValued>false</multiValued>
          <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
          <name>bilSymSeg_max_occlusion_dist</name>
          <type>Float</type>
          <description>Upper bound for measuring occlusion in a segment</description>
          <multiValued>false</multiValued>
          <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
          <name>bilSymSeg_min_perpendicular_angle</name>
          <type>Float</type>
          <multiValued>false</multiValued>
          <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
          <name>bilSymSeg_max_perpendicular_angle</name>
          <type>Float</type>
          <multiValued>false</multiValued>
          <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
          <name>correspondence_max_sym_reflected_dist</name>
          <type>Float</type>
          <description>Maximum distance between reflected point and other point over symmetry plane to be correspondence</description>
          <multiValued>false</multiValued>
          <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
          <name>symmetric_weight_factor</name>
          <type>Float</type>
          <description>Scaling weight of symmetric neighbors for favoring keeping corresponding points</description>
          <multiValued>false</multiValued>
          <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
          <name>bilSymSeg_fg_weight_factor</name>
          <type>Float</type>
          <description>Scaling weight of foreground point for keeping more points in segment</description>
          <multiValued>false</multiValued>
          <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
          <name>bilSymSeg_bg_weight_factor</name>
          <type>Float</type>
          <description>Scaling weight of background point for cutting more points in segment</description>
          <multiValued>false</multiValued>
          <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
          <name>bilSymSeg_max_sym_score</name>
          <type>Float</type>
          <description>Maximum bound for passing segments more symmetric property </description>
          <multiValued>false</multiValued>
          <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
          <name>bilSymSeg_max_occlusion_score</name>
          <type>Float</type>
          <description>Maximum bound for passing segments with enough point closeness </description>
          <multiValued>false</multiValued>
          <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
          <name>bilSymSeg_max_cut_score</name>
          <type>Float</type>
          <description>Maximum bound for passing segments enough cut score per segment point</description>
          <multiValued>false</multiValued>
          <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
          <name>min_sym_sypport_overlap</name>
          <type>Float</type>
          <description>Overlap ratio between compute segment and symmetry support to filter out invalid segments</description>
          <multiValued>false</multiValued>
          <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
          <name>min_segment_size</name>
          <type>Integer</type>
          <description>Minimum segment size to pass</description>
          <multiValued>false</multiValued>
          <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
          <name>overlap_threshold</name>
          <type>Float</type>
          <description>Overlap Ratio between two segment to be merged</description>
          <multiValued>false</multiValued>
          <mandatory>true</mandatory>
      </configurationParameter>
    </configurationParameters>
    <configurationParameterSettings>
