        <featureDescription>
          <name>center</name>
          <description>Center of mass</description>
          <rangeTypeName>rs.cv.Point2d</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>axisX</name>
          <description>Direction of x-axis scaled to the length in x direction</description>
          <rangeTypeName>rs.cv.Point2d</rangeTypeName>
