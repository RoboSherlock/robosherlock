          <name>contour</name>
          <description>Contour points of segment</description>
          <rangeTypeName>uima.cas.FSArray</rangeTypeName>
          <elementType>rs.cv.Point</elementType>
        </featureDescription>
        <featureDescription>
          <name>children</name>
          <description>Segments inside this segment</description>
          <rangeTypeName>uima.cas.FSArray</rangeTypeName>
          <elementType>rs.core.Annotation</elementType>
        </featureDescription>
        <featureDescription>
          <name>area</name>
          <description>Area in pixles</description>
          <rangeTypeName>uima.cas.Long</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>childrenArea</name>
          <description>Area of inside segments</description>
          <rangeTypeName>uima.cas.Long</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>holes</name>
          <description>Number of inside segments</description>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>rect</name>
          <description>Axis alignd bounding rect</description>
          <rangeTypeName>rs.cv.Rect</rangeTypeName>
        </featureDescription>
        <featureDescription>
