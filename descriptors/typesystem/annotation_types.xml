        <featureDescription>
          <name>rotation</name>
          <description>Rotation of 3D pose</description>
          <rangeTypeName>rs.cv.Mat</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>translation</name>
          <description>Translation of 3D pose</description>
          <rangeTypeName>rs.cv.Mat</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>lengthX</name>
          <description>Length in x direction in 3D space</description>
          <rangeTypeName>uima.cas.Double</rangeTypeName>
        </featureDescription>
        <featureDescription>
